<%= form_for @event do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <br>
  <%= form.label :date %>
  <%= form.text_field :date %>
  <br>
  <%= form.label :location %>
  <%= form.text_field :location %>
  <br>
  <%= form.label :cost %>
  <%= form.text_field :cost %>
  <br>
  <%= form.submit 'Update' %>

<% end %>